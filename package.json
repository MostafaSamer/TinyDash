  "repository": {
    "type": "git",
    "url": "https://github.com/MostafaSamer/TinyDash.git"
  },
  "bugs": {
    "url": "https://github.com/MostafaSamer/TinyDash/issues"
  },
  "homepage": "https://github.com/MostafaSamer/TinyDash#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }

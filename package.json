{
  "name": "@shockwavejs/tinydash",
  "version": "1.3.0",
  "description": "A comprehensive utility library for arrays, objects, strings, numbers, and validation in TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --watch",
